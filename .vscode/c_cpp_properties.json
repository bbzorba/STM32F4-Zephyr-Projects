{
  "version": 4,
  "configurations": [
    {
      "name": "Zephyr (stm32f4_disco / LIS302_accel_test)",
      "intelliSenseMode": "linux-gcc-arm",
      "compilerPath": "${env:ZEPHYR_SDK_INSTALL_DIR}/arm-zephyr-eabi/bin/arm-zephyr-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "includePath": [
        "${workspaceFolder}/applications/LIS302_accel_test/inc",
        "${workspaceFolder}/zephyr/include",
        "${workspaceFolder}/zephyr/include/zephyr",
        "${workspaceFolder}/applications/LIS302_accel_test/build/zephyr/include/generated",
        "${workspaceFolder}/applications/LIS302_accel_test/build/zephyr/include/generated/zephyr"
      ],
      "defines": [
        "__ZEPHYR__=1"
      ],
      "compileCommands": "${workspaceFolder}/applications/LIS302_accel_test/build/compile_commands.json"
    }
  ]
}
